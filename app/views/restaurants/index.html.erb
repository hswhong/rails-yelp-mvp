<h1>All Restaurants</h1>

<div>
  <ul>
    <% @restaurants.each do |restaurant| %>
      <li>
        <p><strong><%= link_to restaurant.name, restaurant_path(restaurant) %></strong></p>
      </li>
    <% end %>
  <ul>
</div>

<%= button_to "Add a new restaurant", new_restaurant_path, method: :get %>
